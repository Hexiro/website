{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "importOrder": [
        "^(@/theme)|(@stitches/react(/.*|))$",
        "^(react|next)(/.*|)$",
        "^(@/commons)((/.*|))$",
        "^(@/components/)(ui|layout)((/.*|))$",
        "^(@/components)((/.*|))$",
        "^(@/data)((/.*|))$",
        "^(@)((/.*|))$",
        ".*"
    ],
    "importOrderSeparation": true,
    "quoteProps": "preserve",
    "arrowParens": "always"
}
